{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/**/*.tsx",
    "src/app/**/*.ts",
    "!src/app/**/*.test.ts",
    "!src/app/**/*.test.tsx"
  ],
  "project": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "!src/**/*.test.ts",
    "!src/**/*.test.tsx"
  ],
  "ignore": [
    "**/*.d.ts",
    "node_modules/**",
    ".next/**",
    "out/**",
    "build/**",
    "dist/**",
    "logs/**",
    "closed-tickets/**",
    "open-tickets/**"
  ],
  "ignoreDependencies": [
    "postcss",
    "tailwindcss",
    "eslint-config-next"
  ],
  "next": {
    "entry": [
      "next.config.ts",
      "src/app/**/page.tsx",
      "src/app/**/layout.tsx",
      "src/app/**/error.tsx",
      "src/app/**/loading.tsx",
      "src/app/**/route.ts",
      "src/app/**/default.tsx"
    ]
  }
}